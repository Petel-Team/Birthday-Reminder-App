<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded">
    <StackLayout>
        <Button text="Add item" tap="{{addItem}}"/>
        <ListView items="{{items}}">
            <ListView.itemTemplate>
                <StackLayout>
                    <Label text="{{time}}"/>
                </StackLayout>
            </ListView.itemTemplate>
        </ListView>
    </StackLayout>
</Page>