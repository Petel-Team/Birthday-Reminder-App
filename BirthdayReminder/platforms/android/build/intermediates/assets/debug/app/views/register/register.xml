<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded">
    <StackLayout>
        <TextField id="username" hint="Username (5-10)" width="400" height="100"/>
        <TextField id="password" hint="Password (5-10)" width="400" height="100"/>
        <TextField id="confirmPassword" hint="Confirm Password (5-10)" width="400" height="100"/>
        <Button id="registerButton" text="Register" />
        <Label id="label" text="REGISTER"/>
    </StackLayout>
</Page>